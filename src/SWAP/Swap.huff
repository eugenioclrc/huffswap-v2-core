    dup5                        // [reserve1, reserve0, blockTimestampLast, address(), TOKEN0, amount1Out, amount0Out, to, lt(amount0Out, reserve0), reserve1, amount1Out]
    lt                          // [reserve1, reserve0, blockTimestampLast, address(), TOKEN0, amount1Out, amount0Out, to, lt(amount0Out, reserve0), lt(amount1Out, reserve1)]
    or                          // [reserve1, reserve0, blockTimestampLast, address(), TOKEN0, amount1Out, amount0Out, to, or(lt(amount1Out, reserve1), lt(amount0Out, reserve0))]
