    0x44                        // [packedReserveSlot, reserve0, reserve0, address(), TOKEN0, amount1Out, amount0Out, ADDRESS_MASK, 0x44]
    calldataload                // [packedReserveSlot, reserve0, reserve0, address(), TOKEN0, amount1Out, amount0Out, ADDRESS_MASK, calldataload(0x44)]
    and                         // [packedReserveSlot, reserve0, reserve0, address(), TOKEN0, amount1Out, amount0Out, to]
    dup8                        // [packedReserveSlot, reserve0, reserve0, address(), TOKEN0, amount1Out, amount0Out, to, packedReserveSlot]
    _GET_RESERVE1()             // [packedReserveSlot, reserve0, reserve0, address(), TOKEN0, amount1Out, amount0Out, to, reserve1]
